  "version": "0.2.0",
  "configurations": [
    {
      "name": "elvan-monorepo",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "elvan",
      "cwd": "apps/elvan",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "elvan (profile mode)",
      "cwd": "apps/elvan",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "elvan (release mode)",
      "cwd": "apps/elvan",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "elvan_admin",
      "cwd": "apps/elvan_admin",
      "request": "launch",
      "type": "dart",
       "args": [
        "-d",
        "chrome"
      ]
    },
    {
      "name": "elvan_admin (profile mode)",
      "cwd": "apps/elvan_admin",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
       "args": [
        "-d",
        "chrome"
      ]
    },
    {
      "name": "elvan_admin (release mode)",
      "cwd": "apps/elvan_admin",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
       "args": [
        "-d",
        "chrome"
      ]
    },
    {
      "name": "elvan_shared",
      "cwd": "packages/elvan_shared",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "elvan_shared (profile mode)",
      "cwd": "packages/elvan_shared",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "elvan_shared (release mode)",
      "cwd": "packages/elvan_shared",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}